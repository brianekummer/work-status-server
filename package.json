{
  "name": "work-status-server",
  "version": "1.0.0",
  "description": "Work Status web server",
  "homepage": "http://192.168.1.202:3000",
  "main": "app.js",
  "scripts": {
    "build": "tsc && npm run copy-views && npm run copy-public && npm run copy-files",
    "copy-views": "cp -r src/views dist/src",
    "copy-public": "cp -r public dist/",
    "copy-files": "cp -r files dist/",
    "lint": "eslint \"./**/*.{ts,tsx}\" --ignore-pattern \"dist\" --ignore-pattern \"public/**/*.js\""
  },
  "dependencies": {
    "csv-parser": "^3.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-prefer-import": "^0.0.1",
    "express": "4.18.2",
    "luxon": "3.4.4",
    "mustache-express": "^1.3.2",
    "node-fetch": "^2.6.6",
    "node-watch": "^0.7.4",
    "replace-require-with-import": "^2.1.0",
    "ts-jest": "^29.2.5",
    "ts-migrate": "^0.1.35",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.4.2",
    "@types/mustache-express": "^1.2.5",
    "@types/node": "^22.10.10",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.19.0"
  },
  "author": "Brian Kummer",
